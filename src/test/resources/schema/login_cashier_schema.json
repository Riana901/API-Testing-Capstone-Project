{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Welcome",
  "definitions": {
    "Welcome": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "meta": {
          "$ref": "#/definitions/Meta"
        },
        "results": {
          "$ref": "#/definitions/Results"
        }
      },
      "required": [
        "meta",
        "results"
      ],
      "title": "Welcome"
    },
    "Meta": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "success": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        }
      },
      "required": [
        "message",
        "success"
      ],
      "title": "Meta"
    },
    "Results": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "fullname": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "token": {
          "type": "string"
        }
      },
      "required": [
        "fullname",
        "token",
        "username"
      ],
      "title": "Results"
    }
  }
}
